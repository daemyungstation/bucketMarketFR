@charset "utf-8";
.contain-contract-title { position:relative; text-align:center; padding:20px 0; background:#fd5490; color:#fff; font-size:22px; font-weight:bold; }
.contain-contract-title .contain-contract-title-price { color:#000; font-size:20px; }
.contain-contract-title .contain-contract-title-desc { color:#000; font-size:18px; }
.contain-contract-title2 { position:relative; text-align:center; padding:20px 0; background:#cbcbcb; color:#000; font-size:22px; font-weight:bold; }
.contain-contract-title2 .contain-contract-title-price { color:#000; font-size:20px; }
.contain-contract-title2 .contain-contract-title-desc { color:#000; font-size:18px; }

.contain-contract-desc { position:relative; text-align:center; padding:20px 0; font-size:18px; }
.contain-contract-desc2 { position:relative; text-align:center; padding:10px 0; font-size:14px; }
.contain-contract-guide { text-align:center; font-size:20px; }
.contain-contract-guide .contain-contract-guide-bold { font-size:22px; color:#fd5490; font-weight:bold; }

.box-detail-product11 {position:relative;margin:10px 15px 0 15px;padding:8px 0;}
.box-detail-product11 .detail1 {margin-bottom:8px;font-size:22px;}
.box-detail-product11 .detail2 {text-align:right;font-size:20px;color:#fd5490;font-weight:700;}
.box-detail-product11 .detail3 {/* position:absolute;left:0;bottom:8px; */font-size:15px;color:#9e9e9e;}
.box-detail-product12 {margin:0 15px 10px 15px;padding:8px 0;border-top:1px solid #f0f0f0;}
.box-detail-product12 .detail1 {font-size:15px;}
.box-detail-product12 .detail2 {font-size:18px;}

.contain-step {overflow:hidden;height:60px;border-bottom:1px solid #eaeaea;
    -webkit-backface-visibility: hidden;
    -moz-backface-visibility: hidden;
    backface-visibility: hidden;
}
.contain-step .fixed-box-button {display:none;}
.contain-step.on {height:auto;}
.contain-step.on .fixed-box-button {display:block;}
.contain-step.border { border:none; }
.contain-step.complete {height:60px;}

.contain-step .title-step-order {background:#f0f0f0;}
.contain-step.on .title-step-order {background:#ffffff;}
/*
.title-step-order::after {content:'';display:block;position:absolute;right:15px;top:18px;width:30px;height:25px;font-size:15px;color:#979797;text-align:right;line-height:25px;background:url(../images/img_accordion.png) no-repeat right 50%;background-size:25px auto;}
.contain-step.on .title-step-order::after {background:url(../images/img_accordion_on.png) no-repeat 50%;background-size:25px auto;}
*/
.contain-step.complete .title-step-order::after {content:'완료';background:none;}
.contain-step .box-inner {padding:25px 35px 15px 35px;background:#f8f8f8;}
.contain-step .box-inner2 {padding:25px 35px 15px 35px;background:#ffffff;}

span.btn-type-allagree {height:auto; border-bottom: 1px solid #666666; padding-bottom:10px; margin-bottom:15px; border-radius: 0px; display: flex; flex-wrap: wrap;}
span.btn-type-allagree button.btn-type-allagree-txt {width: calc(100% - 20px);height:20px;text-align:left;font-size:13px;font-weight:700;line-height:20px;}
span.btn-type-allagree button.btn-type-allagree-txt::before {content:'';display:inline-block;width:18px;height:18px;margin:0 7px 0 0px;vertical-align:middle;background:url(../images/img_checkbox4_unchecked.png) no-repeat 50%;background-size:18px auto; vertical-align: top;}
span.btn-type-allagree button.btn-type-allagree-txt.checked::before {background:url(../images/img_checkbox4_checked.png) no-repeat 50%;background-size:18px auto;}
span.btn-type-allagree button.allagree-expend {position:relative;width: 20px;height: 20px;}
span.btn-type-allagree button.allagree-expend span {width: 10px;height: 10px;border-top:2px solid #121212;border-right:2px solid #121212;display: inline-block;transform: rotate(135deg);position:absolute;top: 0px;left: 4px;}

span.btn-type-allagree p { padding-left:25px; width:100%; }

.contain-step .box-agreement {padding:0px;}
.contain-step .box-agreement .form-checkbox {display:block;margin:0 0 0px 0;font-size:14px;}
.contain-step .box-agreement .form-checkbox:before {margin-right:4px; width: 18px; height: 18px; vertical-align: middle; background: url(../images/img_checkbox4_unchecked.png) 50% no-repeat; background-size: 18px auto;}
.contain-step .box-agreement .form-checkbox.checked:before {background:url(../images/img_checkbox4_checked.png) 0 0 no-repeat; background-size:100%;}

/* .contain-step .box-agreement .form-checkbox::after {content:'';display:block;position:absolute;left:0;top:0;right:0;bottom:0;background:url(../images/img_icon_link1.png) no-repeat right 50%;background-size:20px auto;} */
.contain-step .box-agreement .form-checkbox i {font-style:normal;color:#999; margin-right:4px;}
.contain-step .box-agreement .form-checkbox .necessary {color:#fd5490;}
.contain-step .box-agreement .act-agreement {position:relative;}
/* .contain-step .box-agreement .act-agreement::before {content:'';display:block;position:absolute;left:0;top:0;right:0;bottom:0;z-index:1;background:url(../images/img_icon_link1.png) no-repeat right 50%;background-size:20px auto;} */
.contain-step .box-agreement .act-agreement .act-agreement-inner {display:flex;align-items: center;flex-wrap: wrap; background:#f6f6f6; border-radius:5px; padding:5px 10px 8px 10px; margin-top:10px;}
.contain-step .text-information {margin-bottom:20px;font-size:15px;text-align:center;}
.contain-step .box-agreement .btn-agree-check { margin-left:auto; }
.contain-step .box-agreement .btn-agree-check button {position:relative;width: 20px;height: 20px;}
.contain-step .box-agreement .btn-agree-check button span {width: 10px;height: 10px;border-top:2px solid #121212;border-right:2px solid #121212;display: inline-block;transform: rotate(135deg);position:absolute;top: 0px;left: 4px;}

.term-area-content-layer {width:100%;padding: 0px;max-height:470px;margin: 0;overflow: hidden; display:none;margin-top: 10px;}
.term-area-content-layer .term-area-inner {padding: 0 5px 0 0; margin:0; overflow-y: auto;overflow-x: hidden; height:400px;}
.term-area-content-layer .term-area-inner .head-title {display: block; font-size: 14px; color: #222; margin: 0 0 6px 0; text-align:left;}
.term-area-content-layer .term-area-inner .box-agreement {margin: 0 0 14px 0; padding: 0; background: #ffffff; box-sizing: border-box;display:block;text-align: left;}
.term-area-content-layer .term-area-inner .box-agreement .box-privacy {padding:0;}
.term-area-content-layer .term-area-inner .table-title {font-size: 14px; color: #222; margin: 0 0 6px 0; font-weight: 300; text-align:left;}
.term-area-content-layer .content-terms { padding:0; }

.step-certification {text-align:center;}
.step-certification li {position:relative;display:inline-block;margin:0 10px;width:85px;font-size:14px;text-align:center;vertical-align:top;color:#9e9e9e;}
.step-certification li::after {content:'';display:block;position:absolute;left:-13px;top:37px;width:10px;height:18px;background:url(../images/img_arrow_step.png) no-repeat 50%;background-size:10px auto;}
.step-certification li:first-child::after {content:none;}
.step-certification .step1::before {content:'';display:block;margin:0 auto 12px auto;width:83px;height:83px;background:#fff url(../images/img_step_certification1.png) no-repeat 50%;background-size:40px auto;border-radius:100%;}
.step-certification .step2::before {content:'';display:block;margin:0 auto 12px auto;width:83px;height:83px;background:#fff url(../images/img_step_certification2.png) no-repeat 50%;background-size:51px auto;border-radius:100%;}
.step-certification .step3::before {content:'';display:block;margin:0 auto 12px auto;width:83px;height:83px;background:#fff url(../images/img_step_certification3.png) no-repeat 50%;background-size:31px auto;border-radius:100%;}

.box-certification-kakao-complete {padding:70px 0 30px 0;}
.box-certification-kakao-complete .text-information::before {content:'';display:block;margin:0 auto 12px auto;width:83px;height:83px;background:#fff url(../images/img_step_certification3.png) no-repeat 50%;background-size:31px auto;border-radius:100%;}

.contain-information-payment-layer {padding:20px 15px;}
.contain-information-payment-layer .text-normal-type1 {font-size:12px;color:#999;}

.form-birthday.type2 input {width: calc(100% - 140px);}
/* .box-information-product1 dt {width: 100%; }
.box-information-product1 dd {width: 100%; flex:2;} */

/*****************390이하******************************************/
@media all and (max-width:390px){
.contain-step .box-inner {padding:25px 15px 15px 15px;}
.step-certification li {width: 90px; margin: 0 8px;}
.step-certification .step1::before {width: 75px; height: 75px;}
.step-certification .step2::before {width: 75px; height: 75px;}
.step-certification .step3::before {width: 75px; height: 75px;}
.step-certification li::after {top: 27px; left:-15px;}



}

/*****************355이하******************************************/
@media all and (max-width:355px){
.step-certification li {width: 75px;}
