/* 개인정보처리방침-html */
.content-privacy .text-normal-type1,
.content-terms .text-normal-type1 {font-size:14px; line-height:24px;}
.box-privacy-number {border: 1px solid #ddd; padding: 15px 40px; overflow: hidden; margin:30px 0 0 0; box-sizing: border-box;}
.box-privacy-number a {width:49%; display: inline-block; font-size:14px; color: #777; margin: 10px 0; padding-right:20px; box-sizing: border-box; vertical-align: top;}
.box-privacy-line {border: 1px solid #ddd; padding: 20px 30px; overflow: hidden; margin:30px 0; box-sizing: border-box;}
.box-top-line {border: 1px solid #ddd; padding: 20px 30px; overflow: hidden; margin:0 0 30px 0; box-sizing: border-box;  line-height:20px;}
.box-top-line-none {padding: 20px 0px; overflow: hidden; margin:0 0 30px 0; box-sizing: border-box;  line-height:20px;}
.box-privacy {margin: 0 0 0px 0; padding:55px 0 0 0;}
.box-privacy dt {color: #364d8e; font-size:16px; line-height:26px; margin: 0 0 10px 0;}
.box-privacy dd {font-size:14px; color: #888; line-height:24px; box-sizing: border-box; padding-left:15px;}
.box-privacy dd ul {margin: 15px 0 30px 0;}
.box-privacy dd ul li {margin: 15px 0;}
.box-privacy dd .table-type2 {border-top:none; margin: 10px 0;}
.box-privacy dd .table-type3 {border-left:1px solid #e1e1e1; border-top:1px solid #e1e1e1; margin: 10px 0;}
.box-privacy dd .table-type3 th,
.box-privacy dd .table-type3 td {font-size:13px; line-height:18px; vertical-align: middle; border-right:1px solid #e1e1e1;}
.box-privacy dd .table-type3 tbody th {background: #fff;}
.box-privacy dd .table-type3 td {text-align: left; padding: 11px 20px;}
.box-privacy dd .table-type3 td a {color: #888;}
.box-privacy dd .table-type3.type2 td {text-align: center;}
.box-privacy .privacy-inner1 {display: block; margin: 0 20px 20px 20px;}
.box-privacy .privacy-inner2 {display: block; margin: 0 20px 15px 20px;}
.privacy-title {font-size:18px; font-weight:700; line-height:22px; padding-bottom:12px; margin:50px 0 25px 0; display:block; border-bottom:2px solid #222;}
.text-privacy {font-size:14px; color: #888; line-height:24px; box-sizing: border-box; padding-left:0px;}
/* 이용약관-html */
.content-terms .box-privacy {margin: 0 0 30px 0; padding:0;}
