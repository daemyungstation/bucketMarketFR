/* 게시판-글쓰기폼-커뮤니티 */
.contain-board-write {margin:30px auto 30px;}
.contain-board-write .box-write-title {position: relative;}
.contain-board-write .box-write-title .subject {width:900px; height:56px; margin-bottom:13px; text-indent: 30px; font-size:15px;}
.contain-board-write .box-write-title .date {font-size: 15px; color: #999;}
.contain-board-write .box-write-title .name {position: absolute; right: 0; top: 0; z-index: 10; display: table;}
.contain-board-write .box-write-title .name img {width: 56px; height:56px; margin-right: 8px; border-radius: 100%;}
.contain-board-write .box-write-title .name span {display: table-cell; vertical-align: middle; color:#364d8e; font-size:15px;}

.contain-board-write .box-write-content {margin: 30px 0 0 0;}
.contain-board-write .form-textarea {height:500px;}
.contain-board-write .form-textarea textarea{padding:30px; height:100%; font-size:15px;}
.contain-board-write .form-textarea i {font-size:15px; right:30px; bottom:20px;}

.contain-board-write .box-board-btn {text-align: right; margin: 20px 0;}
.contain-board-write .box-board-btn span[class^='btn-type-'] {width:103px; height:40px; display: inline-block; margin:0 0 0 10px; border-radius: 0px; box-sizing: border-box;}
.contain-board-write .box-board-btn span[class^='btn-type-'] button {font-size:15px; line-height: 40px;}
.contain-board-write .box-board-btn span[class^='btn-type-'].btn-type-text7 {background: #fff; border:1px solid #d9d9d9; }
.contain-board-write .box-board-btn span[class^='btn-type-'].btn-type-text7 button{color: #999; }

/* paginate */
.paginate {padding:20px 0;text-align:center;}
.paginate button {margin:0 5px;width:17px;height:17px;text-align:center;line-height:17px;font-size:14px;color:#999;overflow:hidden;text-indent:-1000px;}
.paginate button.on {color:#222;}
.paginate span {display:inline-block;margin:0 10px;}
.paginate span button {text-indent:0;}
.paginate .first {background:url(../images/img_paginate_first.png) no-repeat 50%;background-size:17px auto;}
.paginate .prev {background:url(../images/img_paginate_prev.png) no-repeat 50%;background-size:17px auto;}
.paginate .next {background:url(../images/img_paginate_next.png) no-repeat 50%;background-size:17px auto;}
.paginate .last {background:url(../images/img_paginate_last.png) no-repeat 50%;background-size:17px auto;}

/* button-게시판 */
.box-btn.center {text-align: center;}
.box-btn.right {text-align: right;}
.box-btn.left {text-align: left;}
.box-btn span {display: inline-block; border-radius: 0px;}
.box-btn .btn-type-text1 {width:200px; height:50px;}
.box-btn .btn-type-text1 button {font-size:18px; line-height: 50px;}

/* 키워드 */
.keyword {float:left; display:inline-block; margin: 0 4px 12px 0; background: #f8f8f8; border-radius: 33px;}
.keyword > a {display: block; color: #777; font-size: 16px; letter-spacing: -0.8px; padding: 5px 15px;}
.keyword.on {background-color: #fd5490;}
.keyword.on > a {color: #fff;}
.keyword:hover,
.keyword:active,
.keyword:focus {background-color: #fd5490;}
.keyword:hover a,
.keyword:active a,
.keyword:focus a {color: #fff;}

/* 가입후기 */
.box-latter-head {margin: 30px 15px 0 15px;}
.box-latter-head h2 {font-size:22px; font-weight:400; margin-bottom:8px;}
.box-latter-body {margin:0 0 30px 0;}
.area-inner.latter .box-latter-body {margin-bottom:0;}
.layer-popup1 .box-latter-body {margin: 20px 15px;}
.box-latter-view {position:relative;margin-top:20px; padding-bottom:20px; border-bottom:1px solid #f0f0f0;}
.latter-best {position:absolute;left:0;top:60px;padding:0 5px;height:20px;line-height:20px;font-size:12px;color:#fff;background:#fd5490;}
.latter-profile {position:absolute;left:80px;top:40px;width:60px; height:60px;}
.latter-profile img {width:100%; height:100%; border-radius: 100%;}
.box-latter-best .latter-profile {position:absolute;left:25px;top:20px;width:60px; height:60px;}
.latter-star {padding:0 0 5px 110px;}
.box-latter-best .latter-star {padding:0 0 0 90px;}
.star-gray {display: inline-block; width: 160px; height: 32px; background: url(../images/img_star_gray.png) left top no-repeat; background-size: 100% auto;}
.star-red {display: inline-block; height: 32px; background: url(../images/img_star_red.png) left top no-repeat; background-size: auto 100%;}
.latter-name {padding:5px 0 5px 112px;font-size:15px; color: #999;}
.box-latter-best .latter-name {padding:5px 0 5px 92px;}
.latter-day {margin-left:10px;}
.latter-content {margin:10px 39px;font-size:15px;line-height:160%;}
.box-latter-best .latter-content {margin:10px 20px; height: 50px; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2;}
.latter-product {text-align: right; margin-right:15px;}
.latter-product .text-point1 {display:inline-block; width: calc(100% - 150px); margin: 0 10px 0 39px; font-size:12px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; vertical-align: middle;}
.latter-product .btn-small-box1 {float:right; margin-top:-4px;height:23px; background:#fff; border:1px solid #444; display: inline-block;}
.latter-product .btn-small-box1 a,
.latter-product .btn-small-box1 button {display: block; color:#444; font-size:12px; letter-spacing: -0.6px; padding:0 6px; line-height:23px;}
.latter-photo {margin:22px 15px 0px 39px; text-align: center;}
.latter-photo img {max-width:100%; height:auto;}

.area-content-layer .latter-profile {left:0;top:0;width:35px;height:35px;}
.area-content-layer .latter-star {padding:0 0 0 50px;}
.area-content-layer .star-gray {width: 100px; height: 20px;}
.area-content-layer .star-red {height: 20px;}
.area-content-layer .latter-name {padding:5px 0 5px 53px;font-size:13px;}
.area-content-layer .latter-content {margin:10px 20px 10px 53px;font-size:13px;}
.area-content-layer .latter-photo {margin:15px 20px 0px 50px; text-align: center;}

/* 약관/정책 */
.contain-provision {background: #f6f6f6; padding: 40px 40px; border-top:2px solid #222;}
.contain-provision .box-agreement {background: #f6f6f6 !important; height: auto !important;}
.contain-provision .title {font-size: 14px; font-weight: 700; line-height:24px; margin-bottom: 7px;}
.contain-provision p {font-size:13px; margin-bottom: 30px; line-height:24px;}

.area-top-type1 {background:#ececec;}
.area-top-type2 {width:100%;margin:0 auto;}
