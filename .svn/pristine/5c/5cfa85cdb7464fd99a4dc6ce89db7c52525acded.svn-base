@import url("font.css");
html {-webkit-text-size-adjust: none;-moz-text-size-adjust: none;-ms-text-size-adjust: none;}
html, body {margin:0;padding:0; background-color: #fff;}
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {margin:0; padding:0;}
body {font-weight:300;font-size:14px;color:#454545;font-family:'Noto Sans', Arial, Helvetica, sans-serif;letter-spacing:-0.75px;line-height:120%;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing: grayscale; word-break: break-all;}
table {width:100%;border:0;border-spacing:0;border-collapse:collapse;}
table caption,
form legend{visibility:hidden;width:0;height:0;line-height:0;font-size:0;}
button::-moz-focus-inner {padding:0;border:0;}
button, input[type=submit], input[type=reset], input[type=button] {overflow:visible; margin:0; padding:0; cursor:pointer; border:0; background:transparent;outline:none;}
input[type=checkbox] {-webkit-appearance:checkbox;}
input[type=image]{vertical-align:top;}
input, select, textarea, button {font-family:'Noto Sans', sans-serif;font-weight:300;font-size:13px;border-radius:0;outline:none;}
li {list-style:none;}
img, fieldset {border:none; vertical-align:top;}
img {max-width:100%;}
hr {display:none;}
em, address, cite {font-style:normal;}
strong {font-weight:500;}
table {width:100% !important;}
table th,
table td {font-weight:300;}

ol li {margin:0 0 0 16px;padding:0 0 0 1px;list-style: decimal;}

.contain-loading {display:none;position:fixed;left:0;top:0;right:0;bottom:0;background:rgba(255,255,255,.5);z-index:100;}
.contain-loading.active {display:block;}
.contain-loading svg {display:block;position:absolute;left:50%;top:50%;margin:-50px 0 0 -50px;width:100px;height:100px;}

a {font-size:13px;color:#454545;outline:none;}
a:link {text-decoration:none;}
a:visited {text-decoration:none;}
a:hover, a:focus {text-decoration:none;}/* text-decoration:underline; */
a:active {text-decoration:none;}

/* Form */
label {display:block;margin-bottom:7px;font-size:14px;color:#222;}
input[type^='text'],
input[type^='number'],
input[type^='password'] {display:block;margin:0;padding:0;width:100%;height:44px;text-indent:15px;font-size:16px;color:#222;font-weight:300;border:1px solid #d9d9d9;box-sizing:border-box;outline:none;}
input::placeholder {color:#999; font-weight:300; opacity:  1;}
input::-webkit-input-placeholder {color: #999;} /* Chrome */
input:-mos-input-placeholder {color: #999; opacity: 1;} /* Firefox */
input:-ms-input-placeholder {color: #999;} /* IE */
input::-ms-input-placeholder {color: #999;} /* edge */
input[type^='number']:disabled,
input[type^='text']:disabled {background:#f8f8f8; font-weight:300; color: #999;}
input[type^='number']:disabled::placeholder,
input[type^='text']:disabled::placeholder {color: #888;}
textarea::placeholder {color:#999;font-weight:300;}
select {font-size:14px;vertical-align:middle;}

.box-form {overflow:hidden;margin:10px 0 15px 0;}
.box-form .box-form {overflow:hidden;margin:0;}
.box-form dt {position:relative;margin-bottom:8px;font-size:18px;}
.box-form dt i {font-style:normal;color:#fd5490;}
.box-form dt .select {position:absolute;right:0;top:0;font-size:14px;color:#666;}
.box-form dt.necessary::after {content:'*';display:inline-block;margin:0 0 0 4px;vertical-align:middle;color:#fd5490;}
.box-form dd {position:relative;font-size:21px;}
.box-form dd .form-checkbox {margin-right:10px;}
.box-form dd.box-checkitem .form-checkbox {margin-right:0px;}
.box-form dd.contract .form-select {margin-top:8px;}
.box-form dd.contract .form-select:first-child {margin-top:0;}
.box-form .text-form-notice1 {font-size:12px;color:#999; margin-top:5px; display: block;}
.box-form .text-form-notice2 {font-size:12px;color:#fd5490; margin-top:5px; display: block;}
.box-form dd + dd {margin-top:8px;}
.box-form dt label {font-size:18px;}
.box-form .form-textarea textarea {font-size:18px;}
i > span {color: #222;}

.form-textarea {display:block;position:relative;margin:0;padding:0;}
.form-textarea i {display:block;position:absolute;right:15px;bottom:10px;font-style:normal;font-size:12px;color:#999;font-weight:300;}
.form-textarea textarea {display:block;margin:0;padding:10px 15px;width:100%;height:150px;font-size:16px;color:#222;border:1px solid #d9d9d9;box-sizing:border-box; overflow-y: auto;}

.form-checkbox {display:inline-block; vertical-align:middle; overflow:hidden; position:relative; cursor:pointer; font-size:18px; line-height:29px;color:#333;}
.form-checkbox input[type=checkbox] {position:absolute; left:-20px; top:-20px}
.form-checkbox:before {content:''; display:inline-block; vertical-align:top; width:29px; height:29px; background:url(../images/img_checkbox1_unchecked.png) no-repeat; margin-right:9px; background-size:100%; }
.form-checkbox.checked:before {background:url(../images/img_checkbox1_checked.png) 0 0 no-repeat; background-size:100%; }

.box-checkitem .form-checkbox {width:calc(50% - 5px);height:44px;line-height:44px;color:#999;border:1px solid #d9d9d9; box-sizing: border-box;}
.box-checkitem .form-checkbox.checked {color:#fd5490;border:1px solid #fd5490;}
.box-checkitem .form-checkbox:before {margin:0 0 2px 0;width:40px;height:40px;vertical-align:middle;background:url(../images/img_checkbox2_unchecked.png) 50% no-repeat; background-size: 40px;}
.box-checkitem .form-checkbox.checked:before {background:url(../images/img_checkbox2_checked.png) 50% no-repeat;}

.form-radio {display:block;overflow:hidden;position:relative;cursor:pointer;font-size:18px;line-height:34px;color:#333;}
.form-radio input[type=radio] {position:absolute;left:-20px;top:-20px}
.form-radio:before {content:'';display:inline-block;vertical-align:top;width:16px;height:16px; background:url(../images/img_radio_unchecked.png) 50% no-repeat; margin: 5px 8px 0 0; background-size:100%;}
.form-radio.checked:before {background:url(../images/img_radio_checked.png) 0 0 no-repeat; background-size:100%;}

.box-radio {display:flex;}
.box-radio .form-radio {display:block;flex:auto;margin:0;padding:0;height:44px;font-size:18px;line-height:44px;text-align:center;color:#999;border:1px solid #d9d9d9;box-sizing:border-box;}
.box-radio .form-radio::before {content:none;}
.box-radio .form-radio.checked {color:#fd5490;border:1px solid #fd5490;}

.box-radio-biztype {margin:10px 0;}
.box-radio-biztype .form-radio::before {content:none;}
.box-radio-biztype .form-radio.checked {color:#fd5490;border:1px solid #fd5490;}

.box-radio-biztype {overflow:hidden;}
.box-radio-biztype .form-radio {display:block;width:100%;height:44px;line-height:44px;color:#999;font-size:20px;text-align:center;background:#fff;border-radius:4px;border:1px solid #d9d9d9;box-sizing:border-box;}
.box-radio-biztype .form-radio {float:left;width:30%;}
.box-radio-biztype .form-radio + .form-radio {margin-left:8px;width:calc(70% - 8px);}
.box-radio-biztype .form-radio + .form-radio + .form-radio {margin:0;width:100%;}
.box-radio-biztype .form-radio.checked {color:#fff;background:#fd5490;border:1px solid #fd5490;}


.box-radio-type1 .form-radio {position:relative;margin-bottom:-1px;padding:0 15px;width:auto;height:44px;line-height:44px;color:#999;border:1px solid #d9d9d9;}
.box-radio-type1 .form-radio.checked {z-index:1;color:#fd5490;border:1px solid #fd5490;}
.box-radio-type1 .form-radio:before {position:absolute;right:0;top:50%;margin:-20px 0 2px 0;width:40px;height:40px;vertical-align:middle;background:url(../images/img_checkbox2_unchecked.png) 50% no-repeat; background-size:24px auto;}
.box-radio-type1 .form-radio.checked:before {background:url(../images/img_checkbox2_checked.png) 50% no-repeat; background-size:24px auto;}

.form-select {display:block;margin:0; box-sizing:border-box;-webkit-box-sizing:border-box;border:1px solid #d9d9d9; position:relative; height:44px;background:#fff; }
.form-select:after {content:''; position:absolute; right:10px; top:50%; margin-top:-12px; width:24px; height:24px; background:url(../images/img_selectbox.png) 0 0 no-repeat; background-size:100%;}
.form-select select {display:inline-block;position:relative;z-index:1;width:100%;box-sizing:border-box; padding:0 60px 0 10px; border:0; height:42px; background:none; -webkit-appearance: none; -moz-appearance: none; appearance: none; color:#111; font-size:14px; cursor:pointer; outline:none;vertical-align:middle;}
.form-select select::-ms-expand {display:none}
.form-alert2,
.form-alert,
.box-form label.form-alert,
.box-form dd.form-alert {height:0;overflow:hidden;margin:0;padding:0;font-size:14px;color:#fd5490;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.form-alert::before {content:'';display:block;width:100%;height:3px;}
.form-alert.alert-right {text-align: right; color: #fd5490;}
.form-alert2 {padding-top:3px;}
.form-alert2,
.form-alert.on,
.form-alert.ver2,
.box-form label.form-alert.on,
.box-form dd.form-alert.on,
.box-form label.form-alert.ver2,
.box-form dd.form-alert.ver2 {height:auto; padding-bottom: 3px;}
.form-notice {font-size:14px;color:#fd5490;}
.box-form dd.form-notice {font-size:14px;color:#fd5490;}

div.box-form input{margin-bottom:10px;}
div.box-form .form-alert.ver2,
div.box-form .form-alert.on{margin: -10px 0 10px 0;}

.form-addbutton {display:block;position:relative;}
.form-addbutton input {width:calc(100% - 110px)}
.form-addbutton span[class^='btn-'] {position:absolute;right:0;top:0;width:100px;background:#d9d9d9;border-radius:0;}
.form-addbutton span[class^='btn-'] button {color:#222;font-size:16px;text-align:center;text-indent:0;}

.form-birthday {display:flex;text-align:right;}
.form-birthday input[type^='text'] {flex:auto;}
.form-birthday .form-radio {flex:none;padding-top:10px;padding-right:10px;}

.form-regnum {display:block;position:relative;overflow:hidden;}
.form-regnum i {display:block;float:left;width:20px;text-align:center;line-height:44px;font-style:normal;}
.form-regnum input {float:left;width:calc(50% - 60px);}
.form-regnum span[class^='btn-'] {position:absolute;right:0;top:0;width:90px;background:#d9d9d9;border-radius:0;}
.form-regnum span[class^='btn-'] button {color:#222;font-size:16px;text-align:center;text-indent:0;}

.form-compregnum {display:block;position:relative;overflow:hidden;}
.form-compregnum i {display:block;float:left;width:2%;text-align:center;line-height:44px;font-style:normal;}
.form-compregnum input {float:left;width:32%;}

.form-site {margin: 0 0 2px 15px; color: #999; font-weight: 300;}
.form-site:nth-of-type(1) {margin: 5px 0 2px 15px;}

.box-information-product1 {margin:0 0 20px 0;padding:15px;border:1px solid #d9d9d9;}
.box-information-product1 dl {display:flex;flex-wrap:wrap;}
.box-information-product1 dt {flex:auto;width:60%;font-size:16px;color:#999;}
.box-information-product1 dd {flex:auto;width:40%;text-align:right;font-size:16px;color:#444;}
.box-information-product2 {margin:0 15px 11px 15px;padding:15px;border:1px solid #d9d9d9;}
.box-information-product2 dl {display:flex;}
.box-information-product2 dt {flex:none;width:110px;font-size:14px;color:#999;}
.box-information-product2 dd {flex:auto;text-align:left;font-size:14px;color:#444;}

.text-agreement label {position:absolute;left:0;top:0;}
.text-agreement button {color:#364d8e;text-decoration: underline;}

.text-notice-type2 {font-size:13px;color:#fd5490;}
.text-notice-type2.form {text-align:right;}
.text-notice-type2.form2 {float:right; text-align:right;}
.text-notice-type2.form3 {text-align:right; padding: 20px 0;}

.text-notice-type3 {padding:10px 0;font-size:12px;color:#999;}

/* Layout */
.area-topbanner {height:0;overflow:hidden;}
.area-topbanner.active {height:80px;}
.area-topbanner .area-inner {position:relative;margin:0 auto;text-align:center;}
.area-topbanner .area-inner a {display:block;position:relative;width:auto;height:80px;}/* 0707수정*/
.area-topbanner .area-inner a img {display:block;position:absolute;left:50%;top:0;margin-left:-950px;max-width:none;height:80px;} /* 0707추가*/
.area-topbanner .btn-close-banner {display:block;position:absolute;right:50%;top:50%;margin:-20px -620px 0 0;width:40px;height:40px;background:url(../images/img_btn_header_banner_close.png) no-repeat 50%;}
.area-topbanner .btn-close-banner button {display:block;width:40px;height:40px;text-indent:-1000px;overflow:hidden;}

.area-header {margin-bottom:100px;}
.area-header .contain-util {border-bottom:1px solid #eaeaea;}
.area-header .contain-util .contain-inner {margin:0 auto;width:1280px;text-align:right;line-height:50px;}
.area-header .contain-util .contain-inner a {padding:0 10px;}
.area-header .area-inner {position:relative;margin:0 auto;padding:65px 0;width:1280px;}
.area-header .area-inner h1.title {position:absolute;left:0;top:45px;width:200px;height:78px;text-indent:-1000px;overflow:hidden;background:url(../images/img_logo_pc.png) no-repeat 50%;}
.area-header .contain-search {position:relative;margin:0 auto;width:450px;height:40px;border-bottom:2px solid #222;}
.area-header .contain-search input[type^='text'] {text-indent:10px;width:410px;height:40px;font-size:16px;border:none;background:none;}
.area-header .contain-search .btn-search {position:absolute;right:0;top:0;width:40px;height:40px;background:url(../images/img_header_search.png) no-repeat 50%;}
.area-header .contain-search .btn-search button {display:block;width:40px;height:40px;text-indent:-1000px;overflow:hidden;}
.area-header .box-favword {position:absolute;left:0;top:48px;right:0;z-index:12;overflow:hidden;height:0;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-header .box-favword .box-inner {padding:13px 15px 3px 15px;background:#fff;border:1px solid #d9d9d9;}
.area-header .box-favword .title {margin-bottom:13px;color:#666;}
.area-header .box-favword .tag button {display:inline-block;margin:0 7px 12px 0;padding:0 18px;height:30px;line-height:30px;color:#777;border-radius:15px;background:#f8f8f8;}
.area-header .contain-word {position:absolute;left:875px;top:70px;z-index:12;overflow:hidden;height:36px;background:#fff;border:1px solid #fff;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-header .contain-word.on {border:1px solid #d9d9d9;}
.area-header .contain-word ul {}
.area-header .contain-word li {padding:0;font-size:13px;color:#666;}
.area-header .contain-word li a {display:block;padding:10px 25px 10px 15px;font-size:13px;color:#666;}
.area-header .contain-word li a:hover {color:#fd5490;}

.area-navigation-global {position:absolute;left:0;top:224px;right:0;z-index:11;height:50px;background:#f8f8f8;}
.area-navigation-global .area-inner {position:relative;margin:0 auto;width:1280px;height:50px;}
.area-navigation-global .contain-category {position:absolute;left:0;top:0;}
.area-navigation-global .contain-category .btn-category {display:block;float:left;margin-right:32px;}
.area-navigation-global .contain-category .btn-category button {position:relative;display:block;width:147px;font-weight:300;line-height:50px;text-indent:60px;}
.area-navigation-global .contain-category .btn-category button::before {content:'';position:absolute;left:0;top:0;display:block;width:50px;height:50px;background:url(../images/img_btn-category.png) no-repeat 50%;}
.area-navigation-global .contain-category .btn-category.on button::before {background:#fd5490 url(../images/img_btn-category_on.png) no-repeat 50%;}
.area-navigation-global .contain-category .btn-link {display:block;float:left;}
.area-navigation-global .contain-category .btn-link a {display:block;padding:0 13px;font-weight:700;line-height:50px;}
.area-navigation-global .contain-category .btn-tag {display:block;float:left;}
.area-navigation-global .contain-category .btn-tag a {display:block;padding:0 13px;font-weight:700;color:#fd5490;line-height:50px;}
.area-navigation-global .contain-myinfor {float:right;}
.area-navigation-global .contain-myinfor .box-infor {position:relative;float:right;padding:0 13px;line-height:50px;}
.area-navigation-global .contain-myinfor .box-infor .box-detail::before {content:'';display:inline-block;margin-right:20px;width:1px;height:20px;vertical-align:middle;background:#fd5490;}
.area-navigation-global .contain-myinfor .box-infor .photo {margin-right:10px;line-height:50px;}
.area-navigation-global .contain-myinfor .box-infor .photo img {display:inline-block;margin-top:9px; width: 33px;height: 33px;border-radius: 100%;}
.area-navigation-global .contain-myinfor .box-infor .name {display:inline-block;line-height:50px;}
.area-navigation-global .contain-myinfor .box-infor .name i {font-style:normal;color:#364d8e;}
.area-navigation-global .contain-myinfor .box-infor .name a,
.area-navigation-global .contain-myinfor .box-infor .name button {color:#fd5490;}
.area-navigation-global .contain-myinfor .box-infor .name a::after,
.area-navigation-global .contain-myinfor .box-infor .name button::after {content:'';display:inline-block;margin:0 0 2px 5px;width:9px;height:5px;vertical-align:middle;background:url(../images/img_dropdown_type1.png) no-repeat 50%;}
.area-navigation-global .contain-myinfor .box-planner {float:right;padding:0 13px;line-height:50px;}
.area-navigation-global .contain-myinfor .box-detail-links {position:absolute;right:10px;top:35px;z-index:2;height:0;overflow:hidden;background:#fff;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-navigation-global .contain-myinfor .box-detail-links ul {border:1px solid #d9d9d9;}
.area-navigation-global .contain-myinfor .box-detail-links ul a {display:block;padding:11px 15px;width:115px;line-height:100%;color:#666;}
.area-navigation-global .contain-myinfor .box-detail-links ul a:hover {color:#fff;background:#fd5490;}
.area-navigation-global .contain-myinfor .box-planner {position:relative;}
.area-navigation-global .contain-myinfor .box-planner .detail-planner {}
.area-navigation-global .contain-myinfor .box-planner .detail-planner button {color:#fd5490;}
.area-navigation-global .contain-myinfor .box-planner .detail-planner button::after {content:'';display:inline-block;margin:0 0 2px 5px;width:9px;height:5px;vertical-align:middle;background:url(../images/img_dropdown_type1.png) no-repeat 50%;}
.area-navigation-global .contain-myinfor .detail-planner-links {position:absolute;right:10px;top:35px;z-index:2;height:0;overflow:hidden;background:#fff;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-navigation-global .contain-myinfor .detail-planner-links ul {border:1px solid #d9d9d9;}
.area-navigation-global .contain-myinfor .detail-planner-links ul a {display:block;padding:11px 15px;width:115px;line-height:100%;color:#666;}
.area-navigation-global .contain-myinfor .detail-planner-links ul a:hover {color:#fff;background:#fd5490;}
.area-navigation-global .box-search {display:none;position:relative;float:right;width:50px;line-height:50px;}/* 200622 수정 */
.area-navigation-global .box-search.active {display:block;}/* 200622 추가 */
.area-navigation-global .box-search .btn-searchbox {display:block;width:0px;height:50px;}
.area-navigation-global .box-search .btn-searchbox button {display:block;width:50px;height:50px;text-indent:-1000px;overflow:hidden;background:#fd5490 url(../images/img_gnb_search.png) no-repeat 50%;}
.area-navigation-global .box-search.on .btn-searchbox button {background:#fd5490 url(../images/img_gnb_search_on.png) no-repeat 50%;}
.area-navigation-global .box-search .article-search {position:absolute;right:0;top:60px;z-index:2;width:450px;height:0;overflow:hidden;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-navigation-global .box-search .article-search .contain-inner {padding:20px 25px;border:1px solid #d9d9d9;background:#fff;}
.area-navigation-global .box-search .contain-search {position:relative;margin-bottom:15px;width:auto;border-bottom:2px solid #222;}
.area-navigation-global .box-search .contain-search input[type^='text'] {display:block;margin-right:40px;text-indent:10px;width:355px;height:40px;font-size:16px;border:none;background:none;}
.area-navigation-global .box-search .contain-search .btn-search {position:absolute;right:0;top:0;width:40px;height:40px;background:url(../images/img_header_search.png) no-repeat 50%;}
.area-navigation-global .box-search .contain-search .btn-search button {display:block;width:40px;height:40px;text-indent:-1000px;overflow:hidden;}
.area-navigation-global .box-search .contain-favword {line-height:100%;}
.area-navigation-global .box-search .contain-favword .title {margin-bottom:13px;color:#666;}
.area-navigation-global .box-search .contain-favword .tag button {display:inline-block;margin:0 7px 12px 0;padding:0 18px;height:30px;line-height:30px;color:#777;border-radius:15px;background:#f8f8f8;}

.area-location {position:absolute;left:0;top:274px;right:0;/* background:#f8f8f8; */border-bottom:1px solid #d9d9d9;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.area-location .area-inner {position:relative;margin:0 auto;width:1280px;height:50px;line-height:50px;}
.area-location .area-inner span {display:inline-block;margin-right:7px;min-width:50px;height:50px;vertical-align:top;}
.area-location .area-inner span a::after {content:'>';display:inline-block;margin-left:20px;width:15px;height:15px;font-size:14px;font-family:serif;}/* 200622 추가 */
.area-location .area-inner span:last-child a::after {content:none;}
