div[class^='layer-'] .form-addbutton span[class^='btn-'] {height:44px;}
div[class^='layer-'] .form-addbutton span[class^='btn-'] button {height:44px;line-height:44px;}
.layer-popup1 {position:fixed;left:0;top:100%;right:0;bottom:0;z-index:10;background:#fff;overflow:hidden;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.layer-popup1 .area-header-layer {border-bottom:1px solid #eaeaea;}
.layer-popup1 .area-header-layer .title {text-align:center;line-height:50px;font-size:16px;width:100%; padding: 0 50px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; box-sizing: border-box;}
.layer-popup1 .area-header-layer span[class^='btn'] {display:block;position:absolute;top:13px;width:24px;height:24px;}
.layer-popup1 .area-header-layer span[class^='btn'] button {display:block;width:100%;height:100%;background:none;text-indent:-1000px;overflow:hidden;}
.layer-popup1 .area-header-layer .btn-close-layer {right:15px;background:url(../../images/img_close_layer.png) no-repeat 50%;background-size:20px auto;}
.layer-popup1 .area-content-layer {position:absolute;left:0;top:51px;right:0;bottom:0;padding:0;overflow:auto;}
.layer-popup1 .area-content-layer .area-inner.qna {padding-top:20px;padding-bottom:20px;}
.layer-popup1 .area-content-layer .area-inner.qna .title {/* margin-left:15px; */margin-right:15px;font-weight:normal;}
.layer-popup1 .area-content-layer .area-inner.qna h3.title {margin-left:15px;}
.layer-popup1 .area-content-layer .area-inner.latter {padding-top:20px;padding-bottom:20px;}
.layer-popup1 .area-content-layer .area-inner.latter .title {margin-left:15px;margin-right:15px;margin-bottom:20px;font-weight:normal;}
.layer-popup1 .area-content-layer.bt50 {bottom:50px;}
.layer-popup1.active {top:0;}
.layer-popup1 .box-agreement {padding:15px;height:150px;background:#f8f8f8; overflow-y: auto;}
/* .layer-popup1 .box-agreement.scroll {overflow-y: auto;} */
/* .layer-popup1 .box-agreement > .box-inner > table,
.layer-popup2 .box-agreement > .box-inner > table {width:100% !important;} */
.layer-popup1 .box-agreement.type2 {height:auto;}
.layer-popup1 .box-agreement.full {position:absolute;left:0;top:0;right:0;bottom:0;height:auto;overflow:auto;}
.layer-popup1 .box-agreement.full.hastitle {position:absolute;left:0;top:60px;right:0;bottom:0;height:auto;overflow:auto;}
.layer-popup1 .layer-button-bottom {padding-bottom:60px;}
.layer-popup1 .table-type3 th.on,
.layer-popup1 .table-type3 td.on {color: #fd5490;}

.layer-popup2 {display:none;position:fixed;left:0;top:0;z-index:10;width:100%;height:100%;background:rgba(0,0,0,.7);}
.layer-popup2.active {display:block;}
.layer-popup2 .layer-inner {position:absolute;left:15px;top:100px;right:15px;padding:40px 30px 20px 30px;;background:#fff;border-radius:15px;}
.layer-popup2.infor1 .layer-inner {position:absolute;left:15px;top:100px;right:15px;padding:20px 30px 20px 30px;;background:#fff;border-radius:15px;}
.layer-popup2 .layer-inner .contain-head {padding-bottom:20px;}
.layer-popup2 .layer-inner .contain-body {padding-top:15px;}
.layer-popup2 .layer-inner .btn-close {display:block;position:absolute;right:20px;top:20px;width:20px;height:20px;background:url(../../images/img_close_layer.png) no-repeat 50%;background-size:20px auto;}
.layer-popup2 .layer-inner .btn-close button {display:block;width:20px;height:20px;overflow:hidden;text-indent:-1000px;}
.layer-popup2 .layer-inner .box-button {padding-top:20px;}

