.layer-alert {display:none;position:fixed;left:0;top:0;z-index:10;width:100%;height:100%;background:rgba(0,0,0,.7);}
.layer-alert.active {display:block;}
.layer-alert .layer-inner {position:absolute;left:15px;bottom:60px;right:15px;padding:0;height:40px;color:#fff;font-size:14px;text-align:center;line-height:40px;background:#000;border-radius:20px;}
.layer-alert .layer-inner p {}

/* 스크롤 막기 */
.scrollDisable {height:100%; min-height:100%; overflow:hidden !important; touch-action:none;}

/* 레이어-약관/정책 */
.contain-provision-layer {background: #f6f6f6; padding: 15px 15px;}
.contain-provision-layer h2,
.contain-provision-layer h2.title {color: #222; font-size: 16px; font-weight:700; margin-bottom: 20px; text-align: left; padding:0px;}
.contain-provision-layer .box-agreement {background: #f6f6f6 !important; height: auto !important; padding:0px;}
.contain-provision-layer h3 {font-size: 16px; font-weight: 700; margin-bottom: 8px;}

/* 레이어-배너 */
.layer-popup2.layer-banner .layer-inner{border-radius: 0px; padding:0px; width:320px; left:0px; right:0px; margin: 0 auto;}
.layer-popup2.layer-banner .layer-inner .contain-body {padding:0px; overflow: hidden;}
.layer-popup2.layer-banner .box-layer-banner {margin: 0 auto;  text-align: center; width:320px; height:320px;}
.layer-popup2.layer-banner a {display: block;}
.layer-popup2.layer-banner img {margin:0 auto; vertical-align: top; width:320px; height:320px;}
.layer-popup2.layer-banner .contain-bottom {position: relative; width:100%; height:41px; background: #f8f8f8;}
.layer-popup2.layer-banner .btn-today-close {display:inline-block; margin: 0 0 0 15px; line-height:41px;}
.layer-popup2.layer-banner .btn-today-close button {font-size:14px; color: #222;}
.layer-popup2.layer-banner .btn-close {top:10px; right:15px;}

/* 레이어-앱 */
.layer-popup2.layer-app .layer-inner{border-radius: 0px; padding:0px; background: none;}
.box-layer-app {margin:0 auto; width:320px; padding: 50px 0; position: relative;}
.box-app-top {overflow: hidden; margin: 0 auto; text-align: center;}
.box-app-top img {display: inline-block; width:74px; height:74px;}
.box-app-top .app-title {display: inline-block; vertical-align: top; margin-left:10px; padding-top:3px; color: #fff; font-size:13px; line-height:16px; box-sizing: border-box; text-align: left;}
.box-app-top .app-text {display: block; margin: 6px 0 0 0; font-size:18px; line-height:24px;}
.layer-popup2.layer-app .btn-app-view{display: block; width:190px; height:39px; background: #fd5490; border-radius: 44px; margin:15px auto 0;}
.layer-popup2.layer-app .btn-app-view button {width:100%; height:100%; font-size:14px; color: #fff; text-align: left; padding:0 0 0 16px;}
.layer-popup2.layer-app .btn-app-view button::after {content:'';display:inline-block;width:13px;height:13px;background:url(../../images/img_button_arrow1.png) no-repeat left top; background-size:13px auto; margin: 3px 15px 0 0; vertical-align: middle; float:right;}
.layer-popup2.layer-app .btn-close {right:15px;top:15px;background:url(../../images/img_close_layer_w.png) no-repeat 50%;background-size:20px auto;}
.layer-popup2.layer-app .btn-web-view.btn-close {display: block !important; width:auto; height:auto; text-align: center; position: relative; right:0px; left:0px; top: 0px; margin:20px auto 0px; background: none;}
.layer-popup2.layer-app .btn-web-view.btn-close button {width:auto; height:auto; margin:0 auto; color: #fff; font-size:13px; text-decoration: underline; text-indent: 0px;}

/* 레이어-html가이드 */
.layer-popup2.layer-url-guide {background: none;}
.layer-popup2.layer-url-guide .layer-inner {border:1px solid #222; padding:0px; margin: 40px 0 20px 0; }
.layer-popup2.layer-url-guide .layer-inner .contain-head {padding: 20px 20px 10px 20px;}
.layer-popup2.layer-url-guide .layer-inner .contain-body {text-align: center; overflow: auto; height:410px; padding: 0 10px; box-sizing: border-box;}
