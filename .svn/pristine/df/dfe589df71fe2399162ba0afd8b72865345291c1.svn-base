.divline-type0 {display:block;margin:30px 0;padding:0;width:100%;height:1px;background:none;border:none;}
.divline-type1 {display:block;margin:0;padding:0;width:100%;height:1px;background:#f0f0f0;border:none;}
.divline-type2 {display:block;margin:0;padding:0;width:100%;height:5px;border:none;background:#f0f0f0;}
.divline-type3 {display:block;margin:20px 0;padding:0;width:100%;height:1px;background:#f0f0f0;border:none;}
.divline-type4 {display:block;margin:10px 15px 20px 15px;padding:0;width:auto;height:1px;background:#f0f0f0;border:none;}
.divline-type5 {display:block;margin:10px 0 10px 0;padding:0;width:auto;height:1px;background:none;border:none;}
.divline-type6 {display:block;margin:0 0 20px 0;padding:0;width:100%;height:5px;border:none;background:#f0f0f0;}

div[class^='box-tab-'].swiper-container {display:block;position:static;overflow:hidden;margin:0;padding:0;height:50px;background:#f8f8f8;border-top:1px solid #eaeaea;border-bottom:1px solid #eaeaea;}
div[class^='box-tab-'].swiper-container .swiper-wrapper {width:auto;}
div[class^='box-tab-'].swiper-container .swiper-slide {position:relative;width:auto;box-sizing:border-box;}
div[class^='box-tab-'].swiper-container .swiper-slide button {display:block;padding:0 15px;width:auto;height:50px;font-size:16px;text-align:center;white-space:nowrap;}
div[class^='box-tab-'].swiper-container .swiper-slide.on button {color:#fd5490;font-weight:700;}
div[class^='box-tab-'].swiper-container .swiper-slide.on::after {content:'';display:block;position:absolute;left:0;bottom:0;width:100%;height:1px;background:#fd5490;}

.box-information-type1 {padding:18px 15px;background:#f8f8f8;}
.box-information-type1 p {text-align:center;font-size:12px;color:#fd5490;}

.box-tab-head {display:flex;}
.box-tab-head span {display:block;flex:auto;border-bottom:1px solid #eaeaea;box-sizing:border-box;}
.box-tab-head span button {display:block;position:relative;width:100%;height:53px;font-size:16px;text-align:center;}
.box-tab-head span.on {border-bottom:2px solid #fd5490;}
.box-tab-head span.on button {color:#fd5490;font-weight:700;}

.box-tab-head2 {display:flex;}
.box-tab-head2 span {display:block;flex:auto;border-bottom:1px solid #eaeaea;box-sizing:border-box;}
.box-tab-head2 span button {display:block;position:relative;width:100%;height:53px;font-size:16px;text-align:center;}
.box-tab-head2 span.on {border-bottom:2px solid #fd5490;}
.box-tab-head2 span.on button {color:#fd5490;font-weight:700;}

.box-tab-body {overflow:hidden;height:0;}
.box-tab-body.on {height:auto;}
.box-tab-sub-body {overflow:hidden;height:0;}
.box-tab-sub-body.on {height:auto;}
.box-tab-body .box-inner {padding:15px;}
.box-tab-sub-body .box-inner {padding:15px;}

.box-form {overflow:hidden;margin:10px 0 15px 0;}
.box-form .box-form {overflow:hidden;margin:0;}
.box-form dt {position:relative;margin-bottom:8px;font-size:14px;}
.box-form dt i {font-style:normal;color:#fd5490;}
.box-form dt .select {position:absolute;right:0;top:0;font-size:14px;color:#666;}
.box-form dt.necessary::after {content:'*';display:inline-block;margin:0 0 0 4px;vertical-align:middle;color:#fd5490;}
.box-form dd {position:relative;}
.box-form dd .form-checkbox {margin-right:10px;}
.box-form dd.box-checkitem .form-checkbox {margin-right:0px;}
.box-form dd.contract .form-select {margin-top:8px;}
.box-form dd.contract .form-select:first-child {margin-top:0;}
.box-form .text-form-notice1 {font-size:12px;color:#999; margin-top:5px; display: block;}
.box-form .text-form-notice2 {font-size:12px;color:#fd5490; margin-top:5px; display: block;}
.box-form dd + dd {margin-top:8px}

/* p로 바꿀 경우 */
.box-form > dd > p.form-alert {display: none;}
.box-form > dd > p.form-alert.on:not(:first-of-type) {display: none;}
.box-form > dd > p.form-alert.on {display: block; color: #fd5490;}

/*
.box-form *[class^='form-alert'] {margin:0;}
.box-form *[class^='form-alert'].on {margin-top:3px;margin-bottom:8px;}
*/
.box-form.fixed {position:relative;margin:5px 0;}
.box-form.fixed dt {position:absolute;left:0;top:0;margin:0;padding:0;width:100px;line-height:24px;}
.box-form.fixed dd {margin:0;padding:0 0 0 100px;line-height:24px;font-weight:700;color:#999;}
.box-form dt .text-addtion {position:absolute;right:0;top:0;color:#fd5490;}
.box-radio {margin-bottom:8px;overflow:hidden;}
.box-button {text-align:center;}

.box-order {position:relative;margin-top:8px;padding:12px 15px;background:#fff;border:1px solid #d9d9d9;}
.box-order:first-child {margin:0;}
.box-order .product {display:block;font-size:15px;font-weight:700;}
.box-order .option {display:block;color:#9e9e9e;font-size:14px;}
.box-order .infor {display:block;text-align:right;font-size:14px;}
.box-order .address {display:block;text-align:right;font-size:14px;}
.box-order .btn-type-text5 {position:absolute;right:15px;top:12px;}

.box-movie {position:relative;margin-bottom:20px;min-height:194px;background:#f8f8f8;}
.box-movie::after {content:'';display:block;position:absolute;left:50%;top:50%;margin:-20px 0 0 -20px;width:40px;height:40px;background:#727272 url(../../images/img_media_play.png) no-repeat 13px 50%;background-size:19px auto;border-radius:100%;overflow:hidden;}
.box-movie img {display:block;width:100%;}

.box-quiz {margin-bottom:20px;}
.box-quiz dt {margin-bottom:15px;font-size:15px;font-weight:700; word-break: keep-all;}
.box-quiz dt.necessary::after {content:'*';display:inline-block;margin:0 0 0 4px;vertical-align:middle;color:#fd5490;}
.box-quiz dd {padding:0 0 0 15px;}
.box-quiz > dd > .form-radio {line-height:20px; word-break: keep-all; padding-left: 25px;}
.box-quiz > dd > .form-radio:before {margin: 0px; position: absolute; left:0px; top:2px; width: 18px; height: 18px;}

.box-help {display:none;position:absolute;left:0;top:0;z-index:2;width:auto;}
.box-help::before {content:'';display:block;position:absolute;left:10px;top:-6px;width:7px;height:6px;background:url(../../images/img_box_buloon.png) no-repeat 50%;background-size:7px auto;}
.box-help.rtl::before {content:'';display:block;position:absolute;left:auto;right:10px;top:-6px;width:7px;height:6px;background:url(../../images/img_box_buloon.png) no-repeat 50%;background-size:7px auto;}
.box-help .box-inner {display:block;padding:10px;font-size:13px;color:#fff;background:#797979;border-radius:5px;}

.box-information-searchresult {margin-top:55px; text-align: center;}

.box-error .text-normal-type2 {font-size: 16px; display: block; margin-bottom: 30px;}

.bg-point1 {background-color:#fd5490;}
.bg-point2 {background-color:#364d8e;}
.bg-point3 {background-color:#888888;}