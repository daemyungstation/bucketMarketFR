<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<%-- 로딩 상품 이미지 --%>
<c:set var="loadingProductImage" value="data:image/png;base64,"/>
<%-- 로딩 기획전 이미지 --%>
<c:set var="loadingExhibitionImage" value="data:image/png;base64,"/>
<%-- 로딩 배너 이미지 --%>
<c:set var="loadingBannerImage" value="data:image/png;base64,"/>

<!DOCTYPE html>
<html lang="ko">
<head>
    <%@include file="/WEB-INF/jsp/mo/include/head.jsp" %>
    <link rel="stylesheet" type="text/css" href="<spring:eval expression="@resource['css.ssl.domain']"/>/resources/mo/css/main.css">
</head>
<body>

    <%-- TOP 배너 --%>
    <c:if test="${not empty info.top and cookie.topBannerHide.value ne '1'}">
        <c:set var="top" value="${info.top[0] }"/>
    
        <div class="banner-main-top">
            <a href="javascript:$.requestLink('<c:out value="${top.BNR_MST_URL }"/>', '<c:out value="${top.BNR_MST_TGT }"/>');">
                <img src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${top.CMM_FLE_WEB_PATH }"/>" alt="<c:out value="${top.BNR_MST_TITLE }"/>" onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"/>
            </a>
            
            <span class="btn-close"><button onclick="javascript:$.cookie('topBannerHide', 1, { expires : 1 });">닫기</button></span>
        </div>
        
    </c:if>
    <%-- // TOP 배너 --%>
    
    <%-- 헤더 --%>
    <jsp:include page="/WEB-INF/jsp/mo/include/header.jsp" flush="false">
        <jsp:param name="headerGroup" value="logo|search"/>
    </jsp:include>
    <%-- // 헤더 --%>

    <!-- 본문 -->
    <div class="area-content">
        <div class="area-inner">
    
            <section class="contain-main1">
                <!-- 탭 헤더 : 고정 -->
                <div class="box-tab-head2">
                    <span class="on"><button>버킷마켓</button></span>
                    <span><button onclick="javascript:$.requestPage($.action.myplanner.main.view());">레디플래너</button></span>
                </div>
                <!-- // 탭 헤더 : 고정 -->
                
                <%-- 메인 프로모션 배너 --%>
                <c:if test="${not empty info.promotion }">
                    <div class="box-tab-body tab1 on">
                        <div class="bukkitmarket swiper-container">
                            <div class="swiper-wrapper">
                            
                                <c:forEach var="promotion" items="${info.promotion }">
                                
                                    <div class="swiper-slide">
                                        <a href="javascript:$.requestLink('<c:out value="${promotion.BNR_MST_URL }"/>', '<c:out value="${promotion.BNR_MST_TGT }"/>');">
                                            <img class="lazy"
                                                 src="<c:out value="${loadingProductImage }"/>"
                                                 data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${promotion.CMM_FLE_WEB_PATH }"/>" 
                                                 alt="<c:out value="${promotion.BNR_MST_TITLE }"/>" 
                                                 onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"/>
                                        </a>
                                    </div>
                                    
                                </c:forEach>
                                
                            </div>
                            
                            <%-- 배너가 1개 이상인 경우에만 인디케이터 표시 --%>
                            <c:if test="${fn:length(info.promotion) gt 1 }">
                                <div class="swiper-pagination"></div>
                            </c:if>
                            
                        </div>
                    </div>
                </c:if>
                <%-- // 메인 프로모션 배너 --%>
                
            </section>
            
            <%-- 퀵 배너 --%>
            <%-- 2개 미만인 경우 출력 안함 --%>
            <c:if test="${not empty info.quick and fn:length(info.quick) gt 1 }">
            
                <section class="contain-main2">
                    <div class="box-links">
                    
                        <c:forEach var="quick" items="${info.quick }" varStatus="varStatus">
                        
                            <%-- 4개 까지만 표시 --%>
                            <c:if test="${varStatus.count le 4 }">
                                <span class="btn-link link${varStatus.count }">
                                    <a href="javascript:$.requestLink('<c:out value="${quick.BNR_MST_URL }"/>', '<c:out value="${quick.BNR_MST_TGT }"/>');">
                                        <img src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${quick.CMM_FLE_WEB_PATH }"/>" alt="<c:out value="${quick.BNR_MST_TITLE }"/>" onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"/>
                                    </a>
                                </span>
                            </c:if>
                            
                        </c:forEach>
                    </div>
                </section>
                
            </c:if>
            <%-- // 퀵 배너 --%>
            
            <%-- 기획전 배너 --%>
            <c:if test="${not empty info.exhibition }">
            
                <section class="contain-main3">
                    <h1 class="title">꿈꾸던 삶을 위한 <br >특별 기획전</h1>
        
                    <div class="contain-event-basis">
                    
                        <c:forEach var="exhibition" items="${info.exhibition }" varStatus="varStatus">
                            <div class="box-basis-list">
                                <a href="javascript:$.requestLink('<c:out value="${exhibition.BNR_MST_URL }"/>', '<c:out value="${exhibition.BNR_MST_TGT }"/>');">
                                    <img class="lazy"
                                         src="<c:out value="${loadingExhibitionImage }"/>"
                                         data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${exhibition.CMM_FLE_WEB_PATH }"/>" alt="<c:out value="${exhibition.BNR_MST_TITLE }"/>" 
                                         onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"/>
                                </a>
                            </div>
                        </c:forEach>
                        
                    </div>
                    
                    <span class="btn-more"><a href="javascript:$.requestPage($.action.marketing.plan.list());">더 보기</a></span>
                </section>
                
            </c:if>
            <%-- // 기획전 배너 --%>
            
            <%-- 베스트 상품 --%>
            <c:if test="${not empty info.best and fn:length(info.best) gt 3}">
            
                <section class="contain-main4">
                    <h1 class="title">놓치면 후회 각 <br >베스트 상품</h1>
        
                    <div class="contain-item swiper-container">
                        <div class="swiper-wrapper">
                        
                            <c:forEach var="product" items="${info.best }" varStatus="varStatus">
                            
                                <div class="box-item swiper-slide">
                                    <a href="javascript:$.requestPage($.action.product.basic.view(), { PRD_MST_NO : '<c:out value="${product.PRD_MST_NO }"/>'});">
                                    <dl class="detail-product-type1">
                                        <dd class="image">
                                            <img class="lazy"
                                                 src="<c:out value="${loadingProductImage }"/>"
                                                 data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><spring:eval expression="@file['file.path.product']"/>/<c:out value="${product.PRD_MST_CD }"/>/m_750.png" 
                                                 onerror="this.src='<spring:eval expression="@resource['img.ssl.none.src.mobile']"/>'" 
                                                 alt="">
                                            <c:if test="${not empty product.PRD_MST_SPT_TXT }">
                                                <span class="ribbon-type1">
                                                    <c:out value="${product.PRD_MST_SPT_TXT }"/>
                                                </span>
                                            </c:if>
                                        </dd>
                                        <dt class="catchphrase"><c:out value="${product.PRD_MST_SEL_TXT }"/></dt>
                                        <dd class="detail"><c:out value="${product.PRD_MST_NM }"/></dd>
                                    </dl>
                                    </a>
                                </div>
                            
                            </c:forEach>
                            
                        </div>
                        
                        <%-- 1 페이지 이상인 경우에만 표시 --%>
                        <c:if test="${fn:length(info.best) gt 4}">
                            <div class="swiper-pagination"></div>
                            <div class="swiper-button-prev"></div>
                            <div class="swiper-button-next"></div>
                        </c:if>
                        
                    </div>
        
                    <span class="btn-more"><a href="javascript:$.requestPage($.action.display.product.list());">더 보기</a></span>
                </section>
                
            </c:if>
            <%-- // 베스트 상품 --%>
            
            <%-- 카테고리 --%>            
            <section class="contain-main5">
                <div class="box-links">
                
                    <c:forEach var="category" items="${info.categoryList }" varStatus="varStatus">
                        <span class="btn-link">
                            <a href="javascript:$.requestPage($.action.product.basic.list(), { PRD_CTG_IDX : '<c:out value="${category.PRD_CTG_IDX }"/>'});">
                                <img src="<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/category/<c:out value="${category.PRD_CTG_IDX }"/>.png" alt="<c:out value="${category.PRD_CTG_NM }"/>" >
                                <c:out value="${category.PRD_CTG_NM }"/>
                            </a>
                        </span>
                    </c:forEach>
                    
                </div>
            </section>
            <%-- // 카테고리 --%>
            
            <%-- 키워드 상품 --%>
            <c:if test="${not empty info.keyword and fn:length(info.keyword) gt 3}">
            
                <section class="contain-main6">
                    <h1 class="title"><span class="tag">#냉장고</span>오늘의 버킷 리스트</h1>
        
                    <div class="contain-item swiper-container">
                        <div class="swiper-wrapper">
                            
                            <c:forEach var="product" items="${info.keyword }" varStatus="varStatus">
                            
                                <div class="box-item swiper-slide">
                                    <a href="javascript:$.requestPage($.action.product.basic.view(), { PRD_MST_NO : '<c:out value="${product.PRD_MST_NO }"/>'});">
                                    <dl class="detail-product-type1">
                                        <dd class="image">
                                            <img class="lazy"
                                                 src="<c:out value="${loadingProductImage }"/>"
                                                 data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><spring:eval expression="@file['file.path.product']"/>/<c:out value="${product.PRD_MST_CD }"/>/m_750.png" 
                                                 onerror="this.src='<spring:eval expression="@resource['img.ssl.none.src.mobile']"/>'" 
                                                 alt="">
                                            <c:if test="${not empty product.PRD_MST_SPT_TXT }">
                                                <span class="ribbon-type1">
                                                    <c:out value="${product.PRD_MST_SPT_TXT }"/>
                                                </span>
                                            </c:if>
                                        </dd>
                                        <dt class="catchphrase"><c:out value="${product.PRD_MST_SEL_TXT }"/></dt>
                                        <dd class="detail"><c:out value="${product.PRD_MST_NM }"/></dd>
                                    </dl>
                                    </a>
                                </div>
                            
                            </c:forEach>
                         
                        </div>
                        <div class="swiper-button-prev"></div>
                        <div class="swiper-button-next"></div>
                    </div>
        
                    <%-- 인기 키워드 목록 --%>
                    <c:if test="${not empty info.keywordList }">
                        <div class="box-tag">
                            <c:forEach var="keyword" items="${info.keywordList }" varStatus="varStatus">
                                <c:if test="${keyword.PPL_WRD_SCH eq Const.KEYWORD_TYPE_SEARCH }">
                                    <span><a href="javascript:$.requestPage($.action.common.search.list(), { searchPrdMstKwd : '<c:out value="${keyword.PPL_WRD_TEXT }"/>' });">#<c:out value="${keyword.PPL_WRD_TEXT }"/></a></span>
                                </c:if>
                                <c:if test="${keyword.PPL_WRD_SCH eq Const.KEYWORD_TYPE_LINK }">
                                    <span><a href="<c:out value="${keyword.PPL_WRD_URL }"/>">#<c:out value="${keyword.PPL_WRD_TEXT }"/></a></span>
                                </c:if>
                            </c:forEach>
                        </div>
                    </c:if>
                    
                </section>
                
            </c:if>
            <%-- // 키워드 상품 --%>
            
            <%-- 중단 배너 --%>
            <c:if test="${not empty info.middle }">
            
                <section class="contain-main7">
                    <div class="banner1 swiper-container">
                        <div class="swiper-wrapper">
                    
                            <c:forEach var="middle" items="${info.middle }" varStatus="varStatus">
                            
                                <div class="swiper-slide">
                                    <a href="javascript:$.requestLink('<c:out value="${middle.BNR_MST_URL }"/>', '<c:out value="${middle.BNR_MST_TGT }"/>');">
                                        <img class="lazy"
                                             src="<c:out value="${loadingBannerImage }"/>"
                                             data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${middle.CMM_FLE_WEB_PATH }"/>" 
                                             alt="<c:out value="${middle.BNR_MST_TITLE }"/>" 
                                             onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"/>
                                    </a>
                                </div>
                                
                            </c:forEach>
                        
                        </div>
                        
                        <%-- 배너가 1개 이상인 경우에만 인디케이터 표시 --%>
                        <c:if test="${fn:length(info.middle) gt 1 }">
                            <div class="swiper-pagination"></div>
                        </c:if>
                        
                    </div>
                </section>
            
            </c:if>
            <%-- // 중단 배너 --%>
            
            <%-- 추천 상품 --%>
            <c:if test="${not empty info.recommendCategoryList }">
            
                <c:set var="firstCategory" value="${info.recommendCategoryList[info.randmoRecommendCategory] }"/>
            
                <section class="contain-main8">
                    <div class="main-category-more">
                        <button class="btn-category" id="recommendCategoryTitle"><c:out value="${firstCategory.CMN_COM_NM }"/></button>
                        <div class="main-category-inner">
                        
                            <c:forEach var="category" items="${info.recommendCategoryList }">
                                <c:set var="checked" value="${firstCategory.CMN_COM_IDX eq category.CMN_COM_IDX ? 'checked' : '' }"/>
                                <span>
                                    <label class="form-radio-category <c:out value="${checked }"/>">
                                        <input type="radio" name="recommendCategory" data-category="<c:out value="${category.CMN_COM_IDX }"/>" data-name="<c:out value="${category.CMN_COM_NM }"/>">
                                        <c:out value="${category.CMN_COM_NM }"/>
                                    </label>
                                </span>
                            </c:forEach>
                            
                        </div>
                    </div>
                    <h1 class="title">혜택 뚝딱! 쓱싹!</h1>
        
                    <ul class="box-links" id="recommendProductList">
                        <c:forEach var="product" items="${info.recommendProductList }">
                            <li>
                                <a href="javascript:$.requestPage($.action.product.basic.view(), { PRD_MST_NO : '<c:out value="${product.PRD_MST_NO }"/>'});">
                                    <img class="lazy"
                                         src="<c:out value="${loadingProductImage }"/>"
                                         data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><spring:eval expression="@file['file.path.product']"/>/<c:out value="${product.PRD_MST_CD }"/>/m_750.png"
                                         onerror="this.src='<spring:eval expression="@resource['img.ssl.none.src.mobile']"/>'" 
                                         alt="<c:out value="${product.PRD_MST_NM }"/>" >
                                    <span><c:out value="${product.PRD_MST_NM }"/></span>
                                </a>
                            </li>
                        </c:forEach>
                    </ul>
        
                    <span class="btn-link"><a href="javascript:;" id="btnNewRecommendList">새로운 추천 상품 보기</a></span>
        
                </section>
                
            </c:if>
            <%-- // 추천 상품 --%>
            
            <%-- 하단 배너 --%>
            <c:if test="${not empty info.bottom }">
            
                <section class="contain-main9">
                    <div class="banner2 swiper-container">
                        <div class="swiper-wrapper">
                    
                            <c:forEach var="bottom" items="${info.bottom }" varStatus="varStatus">
                            
                                <div class="swiper-slide">
                                    <a href="javascript:$.requestLink('<c:out value="${bottom.BNR_MST_URL }"/>', '<c:out value="${bottom.BNR_MST_TGT }"/>');">
                                        <img class="lazy"
                                             src="<c:out value="${loadingBannerImage }"/>"
                                             data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${bottom.CMM_FLE_WEB_PATH }"/>" 
                                             alt="<c:out value="${bottom.BNR_MST_TITLE }"/>" 
                                             onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"
                                             />
                                    </a>
                                </div>
                                
                            </c:forEach>
                        
                        </div>
                        
                        <%-- 배너가 1개 이상인 경우에만 인디케이터 표시 --%>
                        <c:if test="${fn:length(info.bottom) gt 1 }">
                            <div class="swiper-pagination"></div>
                        </c:if>
                    </div>
                </section>
            
            </c:if>
            <%-- // 하단 배너 --%>
            
            <%-- 공지사항 --%>
            <section class="contain-main10">
                <h1 class="title">공지사항</h1>

                <c:if test="${not empty info.noticeList }">
                
                    <ul class="list-type-notice">
                        <c:forEach var="notice" items="${info.noticeList }" varStatus="varStatus">
                            <li>
                                <a href="javascript:$.requestPage($.action.board.notice.view(), { BRD_NTS_IDX : '<c:out value="${notice.BRD_NTS_IDX }"/>' });">
                                    <c:out value="${notice.BRD_NTS_TITLE }"/>
                                </a>
                            </li>
                        </c:forEach>
                    </ul>
                    
                </c:if>
                
                <div class="box-links">
                    <span class="btn-link"><a href="javascript:$.requestPage($.action.board.faq.list());">자주하는 질문</a></span>
                    <span class="btn-link"><a href="javascript:$.requestPage($.action.board.inquiry.form());">1:1 문의상담</a></span>
                </div>
                <span class="btn-more"><a href="javascript:$.requestPage($.action.board.notice.list());">더 보기</a></span>
            </section>
            <%-- // 공지사항 --%>
    
        </div>
    </div>
    <!-- //본문 -->
    
    <%-- 팝업 --%>
    <c:if test="${not empty info.popupList and cookie.todayCookie.value ne 'done' }">
        
        <c:set var="popup" value="${info.popupList[0] }"/>
    
        <!-- 팝업 : 오늘 하루 동안 띄우지 않기  -->
        <div class="layer-popup2 layer-banner active">
            <div class="layer-inner">
                <div class="contain-body">
                    <div class="box-layer-banner">
                        <a href="javascript:$.requestLink('<c:out value="${popup.POP_MST_LNK }"/>', '<c:out value="${popup.POP_MST_MDE }"/>');">
                            <img src="<spring:eval expression="@resource['cdn.ssl.domain']"/><c:out value="${popup.CMM_FLE_WEB_PATH }"/>" 
                                 onerror="this.src='<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/none.png'"
                                 alt="<c:out value="${popup.POP_MST_TITLE }"/>">
                        </a> 
                    </div>
                </div>
                <div class="contain-bottom">
                    <%-- 오늘 하루만 사용 여부 --%>
                    <c:if test="${popup.POP_MST_DAY_YN eq 'Y' }">
                        <span class="btn-today-close"><button>오늘 하루 동안 띄우지 않기</button></span>
                    </c:if>
                    <%-- // 오늘 하루만 사용 여부 --%>
                    <span class="btn-close"><button>닫기</button></span>
                </div>
            </div>
        </div>
        <!-- //팝업 : 오늘 하루 동안 띄우지 않기  -->
    
    </c:if>

    <!-- 팝업 : APP안내 팝업  -->
    <c:if test="${cookie.appPopupHide.value ne '1' and !fn:contains(header['User-Agent'], 'bucketmarketApp') }">
    
        <div class="layer-popup2 layer-app active">
            <div class="layer-inner">
                <div class="contain-body">
                    
                    <div class="box-layer-app">
                        <p class="box-app-top">
                            <img src="<spring:eval expression="@resource['img.ssl.domain']"/>/resources/mo/images/img_logo_app.png" alt="버킷마켓" />
                            <span class="app-title">
                                꿈꾸던 삶을 담다
                                <span class="app-text">이제 모바일 앱에서<br />
                                <strong class="text-point1">버킷쇼핑</strong>하세요!
                                </span>
                            </span>
                        </p>
                        <span class="btn-app-view"><button>버킷마켓 앱으로 보기</button></span><%-- TODO 김혁 - 앱 링크 추가 --%>
                        <span class="btn-web-view btn-close"><button onclick="javascript:$.cookie('appPopupHide', 1, { expires : 1 });">모바일 웹으로 보기</button></span>
                        <span class="btn-close"><button onclick="javascript:$.cookie('appPopupHide', 1, { expires : 1 });">닫기</button></span>
                    </div>
        
                </div>        
            </div>
        </div>
        
    </c:if>
    <!-- //팝업 : APP안내 팝업  -->
        
    <%-- // 팝업 --%>
    
    <!-- 제품 템플릿 -->
    <script id="recommendProductTemplate" type="text/x-jquery-tmpl">
        {{each(i, row) recommendProductList}}
            <li>
                <a href="javascript:$.requestPage($.action.product.basic.view(), { PRD_MST_NO : '{{= row.PRD_MST_NO }}'});">
                    <img class="lazy"
                         src="<c:out value="${loadingProductImage }"/>"
                         data-src="<spring:eval expression="@resource['cdn.ssl.domain']"/><spring:eval expression="@file['file.path.product']"/>/{{= row.PRD_MST_CD }}/m_750.png"
                         onerror="this.src='<spring:eval expression="@resource['img.ssl.none.src.mobile']"/>'" 
                         alt="{{= row.PRD_MST_NM }}"
                         />
                    <span>{{= row.PRD_MST_NM }}</span>
                </a>
            </li>
        {{/each}}
    </script>
    
    <script src="<spring:eval expression="@resource['js.ssl.domain']"/>/resources/common/js/lib/tmpl/jquery.tmpl.min.js"></script>
    <script src="<spring:eval expression="@resource['js.ssl.domain']"/>/resources/common/js/jquery.cookie.js"></script>
    
    <%@include file="/WEB-INF/jsp/mo/include/footer.jsp" %>
    <%@include file="/WEB-INF/jsp/mo/include/floating.jsp" %>
    <%@include file="/WEB-INF/jsp/mo/include/aside.jsp" %>
    <%@include file="/WEB-INF/jsp/mo/include/layer.jsp" %>
    <%@include file="/WEB-INF/jsp/mo/include/script.jsp" %>
    
    <script>
        $(function () {
            var $areaContent = $(".area-content"),
                $recommendProductList = $areaContent.find("#recommendProductList"),
                $recommendProductTemplate = $("#recommendProductTemplate");
            // 추천 카테고리
            $areaContent.on("click", "input[name='recommendCategory']", function() {
                $(this).closest(".main-category-inner").removeClass("active");
                var category = $(this).attr("data-category");
                var name = $(this).attr("data-name");
                $areaContent.find("#recommendCategoryTitle").text(name);
                // 상품 목록 갱신
                reloadRecommendProductList(category, false);
            });
            // 추천 상품 보기
            $areaContent.on("click", "#btnNewRecommendList", function() {
                var category = $(".form-radio-category.checked").find("input[name='recommendCategory']").attr("data-category");
                // 상품 목록 갱신
                reloadRecommendProductList(category, true);
            });
            // 추천 카테고리 상품 목록 출력
            function reloadRecommendProductList(category, random) {
                var params = {};
                params.CMN_COM_IDX = category;
                params.random = random;
                $.postSyncJsonAjax($.action.main.reload.recommend.product.ajax(), params, function (data) {
                    $recommendProductList.height($recommendProductList.height());
                    $recommendProductTemplate.tmpl(data).appendTo($recommendProductList.empty());
                    // 이미지
                    $recommendProductList.find(".lazy").Lazy({
                        effect: "fadeIn",
                        placeholder : "<c:out value="${loadingProductImage }"/>"
                    });
                });
            }
            // 이미지 로딩
            $areaContent.find(".lazy").Lazy({
                effect: "fadeIn"
            });
        });
    </script>
</body>
</html>