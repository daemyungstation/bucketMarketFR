.campaign-tab-inner .box-campaign.type3 .catchphrase {margin: 15px 0 5px 0;}
.campaign-tab-inner .box-campaign.type3 .content {display: block; overflow: hidden; text-overflow: ellipsis; width:95%; height: 42px; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2; /* line-height:1.4; */}


/* 레이어-지급상세내역 */
.box-reward-provide {margin: 30px 15px 0;}
.box-reward-provide h2 {font-size:22px; margin-bottom: 30px;}
.box-reward-provide .table-type1 th {text-align: center;}
.reward-title {font-size: 14px; margin: 0 15px;}

/* 레이어-나의 노하우/정보 공유하기 */
.box-profile-layer {overflow: hidden;}
.box-profile-layer .profile {float: left; display: table; margin-bottom:20px;}
.box-profile-layer .profile img {width: 75px; height:75px; border-radius: 100%;}
.box-profile-layer .name {display: table-cell; vertical-align: bottom; padding: 0 0 10px 10px; font-size: 18px; color: #364d8e;}
.box-profile-layer .date {float:right; margin-top: 50px; font-size: 12px; color: #999;}
.box-profile-layer input {margin-bottom:20px;}
.box-profile-layer textarea {height: 250px;}

/* 레이어-비밀번호확인 */
.box-password-layer {margin: 20px 15px;}
.box-password-layer .text-information-type2 {color: #222; margin-bottom:20px;}

/* 레디플래너 소개 */
.contain-planner-intro {width: 100%;}

/* 활용가이드 */
.contain-planner-guide {width: 100%;}

/* 준비중 */
.box-update {width:100%; background: #f8f8f8; text-align: center; padding:100px 0;}
.box-update span {display: block; font-size:26px; color: #a7a7a7; margin-top:10px;}


/*****************350이하******************************************/
@media all and (max-width:350px){
.box-form.fixed dd .btn-type-text5.w100 {display: block; float:none; margin-top:5px; width: 95px;}
.box-profile-form {margin:20px 10px;}
.box-form.fixed dt {width:95px;}
.box-form.fixed dd {padding: 0 0 0 95px;}
}

/* URL COPY */
.seller-url-copy { color:#364d8e; text-decoration: underline; margin: 2px 0 0; font-weight:900; }
